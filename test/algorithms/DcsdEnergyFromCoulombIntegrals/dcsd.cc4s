CoulombVertexReader [
  (file "../../resources/CoulombVertex.bin")
] [
  CoulombVertex HoleEigenEnergies ParticleEigenEnergies
  % Uncomment next line to set the read rank. Default value is No.
% (readRank 40)
].

CoulombIntegralsFromVertex [
  CoulombVertex HoleEigenEnergies ParticleEigenEnergies
] [
  % Uncomment next line if you want PPPPIntegrals. Else slicing is used.
% PPPPCoulombIntegrals
  % Uncomment next line if you want PPPHIntegrals. Else a coupled slicing is
  % used via a dressed Coulomb vertex.
% PPPHCoulombIntegrals
  PHPHCoulombIntegrals
  PPHHCoulombIntegrals HHHHCoulombIntegrals
  HHHPCoulombIntegrals
].

DcsdEnergyFromCoulombIntegrals [
  % Uncomment next line if you want PPPP and PPPH Integrals. Else slicing is used.
% PPPPCoulombIntegrals
  % Uncomment next line if you want PPPHIntegrals. Else a coupled slicing is
  % used via a dressed Coulomb vertex.
% PPPHCoulombIntegrals
  % CoulombVertex is needed independent of slicing or not.
  CoulombVertex
  PHPHCoulombIntegrals
  PPHHCoulombIntegrals HHHHCoulombIntegrals
  HHHPCoulombIntegrals
  ParticleEigenEnergies HoleEigenEnergies
  % Sets maximum iterations. Default is 16.
  (maxIterations 16)
  % Uncomment next line to set the slice rank of PPPP and PPPH Integrals. Default is No.
% (sliceRank 40)
] [
  (DcsdEnergy DcsdEnergy) 
  (DcsdSinglesAmplitudes DcsdSinglesAmplitudes)
  (DcsdDoublesAmplitudes DcsdDoublesAmplitudes)
].

TensorWriter [(Data DcsdEnergy)] [].
