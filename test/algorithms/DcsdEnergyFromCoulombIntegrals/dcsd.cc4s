CoulombVertexReader [
  (file "../../resources/CoulombVertex.bin")
] [
  CoulombVertex HoleEigenEnergies ParticleEigenEnergies
].

CoulombIntegralsFromVertex [
  CoulombVertex HoleEigenEnergies ParticleEigenEnergies
] [
  % if PPPPCoulombIntegrals are not given, slicing will be used
% PPPPCoulombIntegrals 
  PHPHCoulombIntegrals
  PPHHCoulombIntegrals HHHHCoulombIntegrals
  HHHPCoulombIntegrals PPPHCoulombIntegrals
].

DcsdEnergyFromCoulombIntegrals [
  % if PPPPCoulombIntegrals are not given, slicing will be used
% PPPPCoulombIntegrals
  CoulombVertex
  PHPHCoulombIntegrals
  PPHHCoulombIntegrals HHHHCoulombIntegrals
  HHHPCoulombIntegrals PPPHCoulombIntegrals
  HoleEigenEnergies ParticleEigenEnergies
] [
  DcsdEnergy DcsdSinglesAmplitudes DcsdDoublesAmplitudes
].

TensorWriter [(Data DcsdEnergy)] [].
